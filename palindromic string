// Sample code to perform I/O:
#include <stdio.h>
#include<string.h>

int main()
{
    char S[101];
    
    int l=0;//low
    int h=strlen(S)-1;//h id high index
    //scanf("%s",&S);
    while(h>1)
    {
            if(S[l++]!=S[h-1])
            {
                printf("NO");
                return 0;
            }
    }
    printf("YES");


}//end main
